<block wx:for="{{actionList}}" wx:key="index">
	<view class="cu-card article {{isCard?'no-card':''}} margin" bindtap="navTo" data-url="/pages/ServiceCenter/activityDetail/activityDetail?id={{item._id}}">
		<view class="cu-item shadow">
			<view class="title">
				<view class="text-cut">{{item.actName}}</view>
			</view>
			<view class="content">
				<view class="desc">
					<view class="character">
						<view wx:if="{{item.teamName.length!=0}}" class="cu-tag bg-red light sm round">{{item.teamName}}</view>
						<view class="cu-tag bg-green light sm round" style="white-space:normal;">{{item.tag}}</view>
						<view>
							<text class="data">{{item.isPintuan}} 截止日期：{{data_Arr[index]}}</text>
						</view>
					</view>
					
				</view>
				
			</view>
		</view>
		
	</view>
	<view class="justify-end grid col-5 bg-white padding-sm margin  margin-bottom-xs margin-top-xs">
						<button class=" sm cu-btn round line-grey {{shadow?'shadow':''}}" bind:tap="Agree" data-id="{{item._id}}">已拒绝发布</button>
	</view>	

</block>

