<!-- 添加时间段模态框 -->
<view class="cu-modal {{modalName=='toPintuan'?'show':''}}">
	<view class="cu-dialog">
		<!-- 标题 -->
		<view class="cu-bar bg-white justify-end">
			<view class="content">请确认您的信息</view>
			<view class="action" bindtap="hideModal">
				<text class="cuIcon-close text-red"></text>
			</view>
		</view>
		<view class="cu-form-group">
			<view class="title">志愿者姓名</view>
			<text>{{myInfo.name}}</text>
		</view>
		<view class="cu-form-group">
			<view class="title">联系电话</view>
			<text>{{myInfo.phone}}</text>
		</view>
		<view class="cu-form-group">
			<view class="title">服务时间段</view>
			<text>{{serviceTimeSpan[idx[0]]['date']+' '+ serviceTimeSpan[idx[0]]['time']}}</text>
		</view>
		<view class="cu-form-group">
			<view class="title">服务岗位</view>
			<text>{{serviceTimeSpan[idx[0]]['positions'][idx[1]]['name']}}</text>
		</view>

		<view wx:if="{{actions.isSubsidy}}" class="cu-form-group">
			<view class="title inteam ">收款单位</view>
			<radio-group bindchange="getbutton">
				<label class="margin-left-sm">
					<radio class="blue sm radio" value="1"></radio>
					<text> 支付宝</text>
				</label>
				<label class="margin-left-sm">
					<radio class="blue sm radio" value="2"></radio>
					<text> 银行卡</text>
				</label>
			</radio-group>

			<!-- <input class="newName text-sm" placeholder="活动为补贴活动以便发放补贴" bindinput="getBankcard">{{bankCard}}</input> -->
		</view>
		<view wx:if="{{payee=='1'}}">
			<view class="cu-form-group" style="justify-content: between;" style="display: flex;align-items: center; ">
				<view class="title outteam">支付宝号码:</view>
				<input class="tel" bindinput="getAli" placeholder="支付宝号仅发放补贴用">
				{{aliPay}}</input>
			</view>
		</view>
		<view wx:if="{{payee=='2'}}">
			<view class="cu-form-group">
				<view class="title">请选择对应银行</view>
				<picker bindchange="BankPickerChange" value="{{BankPickerIndex}}" range="{{picker}}">
					<view class="picker">
						{{BankPickerIndex?picker[BankPickerIndex]:'请选择'}}
					</view>
				</picker>
			</view>
			<view wx:if="{{BankPickerIndex==picker.length-1}}" class="cu-form-group">
				<view class="title">银行名</view>
				<input bindinput="getElseBank" placeholder="×××银行">{{elseBank}}</input>
			</view>
			<view class="cu-form-group" style="justify-content: between;" style="display: flex;align-items: center; ">
				<view class="title outteam">银行卡号</view>
				<input class="tel" bindinput="getBankCardnumber" placeholder="例：6217003810026896707">
				{{bankCardNumber}}</input>
			</view>
		</view>
		<!-- <view wx:for="{{actions.memo}}" wx:key="index" wx:for-item="item" wx:for-index="index" class="cu-form-group">
			<view class="title inteam">{{item}}</view>
			<input class="newName" placeholder="请完善" bindinput="getactName"></input>
		</view> -->
		<!-- 选择框 -->
		<view class="cu-bar bg-white">
			<view class="action margin-0 flex-sub n solid-left" bindtap="hideModal">取消</view>
			<view class="action margin-0 flex-sub  text-green solid-left" bindtap="hideModal" data-target="join">确定</view>
		</view>
	</view>
</view>

