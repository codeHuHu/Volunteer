<view class="cu-bar">
	<view class="action  flex">
		<view>
			<text class="text-xl text-bold text-black bg-oc-lime-0">{{actions.actName}}</text>
			<view class="margin-left cu-tag bg-red light sm round going-on" wx:if="{{actions.isSubsidy=='1'}}">补贴服务</view>
			<view class="margin-left cu-tag bg-green light sm round going-on" wx:if="{{actions.status=='1'}}">服务进行中</view>
			<view class="margin-left cu-tag bg-green light sm round going-on" wx:elif="{{actions.status=='2'}}" style="background-color: gray; color: aliceblue;">服务已结束</view>
			<!-- <view class="cu-tag bg-{{isFull?'blue':'red'}} light xxl round going-on text-bold" wx:if="{{isPintuan=='1'}}">报名{{isDead?(isFull?'已满':'截止'):(isFull?'已满':'中')}}</view> -->
		</view>
	</view>
</view>

<view class="grid col-4 radius-lg padding-left">
	<view class="bg-oc-gray-6 radius padding-tb-xs text-center margin-tb-xs margin-right" style="width: auto; background-color: rgb(78, 187, 78);">
		<view class=" text-sm">
			{{actions.tag}}
		</view>
	</view>
</view>

<view class="margin-sm radius-lg bg-white">
	<view class="solid-bottom text-lg text-bold padding text-black">服务组织</view>
	<view class="padding flex  justify-between">
		<text class="">负责人</text>
		<text class="text-df ">{{actions.holder}} {{actions.phone}}</text>
	</view>
	<view wx:if="{{actions.holderDetail}}" class="padding flex  justify-between">
		<text class="">负责人介绍</text>
		<text class="text-df ">{{actions.holderDetail}}</text>
	</view>
	<view wx:if="{{actions.teamName}}" class="padding flex  justify-between">
		<text class="">队伍名称</text>
		<text class="text-df ">{{actions.teamName}}</text>
	</view>
</view>
<view>
	<view class="margin-sm radius-lg bg-white static">
		<!-- 分享按钮 -->
		<button wx:if="{{!checkMode}}" open-type="share" class="share cu-avatar round xl">
			<text class="iconfont icon-icon-test20 text-green"></text>
		</button>
		<view class="solid-bottom text-lg text-bold padding text-black">{{isPintuan?'报名':'报名'}}要求</view>
		<view wx:if="{{actions.peoType}}" class="padding flex ">
			<view class="flex-sub">
				<text class="">{{isPintuan?'报名':'报名'}}需知</text>
			</view>
			<view class="flex-twice">
				<text class="text-df">{{actions.peoType}}</text>
			</view>

		</view>
		<view class="padding flex justify-between">
			<text class="">{{isPintuan?'报名':'报名'}}截止日期</text>
			<text class="text-df">{{constant.deadTime}}</text>
		</view>
		<view class="padding flex  justify-between">
			<text class="">{{isPintuan==1?'需求':'需求'}}人数</text>
			<text class="text-df"> {{actions.outJoin}}/{{actions.outNum}}</text>
		</view>
	</view>
	<view class="margin-sm radius-lg bg-white static">
		<view class="solid-bottom text-lg text-bold padding text-black">服务时间段</view>
		<view wx:if="{{!checkMode}}">
			<!-- 报名以及取消按钮 -->
			<view wx:if="{{actions.status=='2'}}">
				<button class="signUp cu-avatar round xl bg-yellow animation-{{animation}}" bindtap="toggle" data-class="scale-up" role="button" aria-disabled="false">
					<text class="avatar-text ">服务结束</text>
				</button>
			</view>
			<view wx:else bindtap="showModal" data-target="{{actions.length!=0?(isDead?(isPintuan?'toGroup':'报名已截止无操作'):(isJoin?'toGroup':(isPintuan?'toPintuan':'toJoin'))):'None'}}">
				<button wx:if="{{isDead||isJoin==1}}" class="signUp cu-avatar round xl bg-{{isFull?'purple':'grey'}} animation-{{animation}}" bindtap="toggle" data-class="scale-up" role="button" aria-disabled="false">
					<text class="avatar-text">{{isPintuan?(isJoin||isFull?'群二维码':'报名截止'):'报名截止'}}</text>
				</button>
				<!-- <button wx:elif="{{!isJoin}}" class="signUp cu-avatar round xl bg-green animation-{{animation}}" bindtap="toggle" data-class="scale-up" role="button" aria-disabled="false">
				<text class="avatar-text ">点击{{isPintuan?'报名':'报名'}}</text>
			</button> -->
				<!-- <button wx:elif="{{isJoin}}" class="signUp cu-avatar round xl bg-red animation-{{animation}}" bindtap="toggle" data-class="scale-up" role="button" aria-disabled="false">
					<text class="avatar-text ">取消{{isPintuan?'报名':'报名'}}</text>
				</button> -->
			</view>
		</view>
		<!-- 服务时间段列表 -->
		<view class="cu-list menu" wx:for="{{serviceTimeSpan}}" wx:key="item" wx:for-index="index">
			<view class="cu-item">
				<!-- 时间段 -->
				<view class="content">
					<view class="text-lg text-bold">{{item['time']}}</view>
					<view class="text-sm text-gray">{{item['date']}} </view>
				</view>
				<!-- 折叠箭头 -->
				<view class="action margin-left-lg text-grey" bindtap="click" data-index="{{index}}">
					<text class="cuIcon-{{boxer[index] ? 'unfold' : 'right'}}">{{item['checked'] ? '收回' : '展开'}}</text>
				</view>
			</view>
			<!-- 岗位内容 -->
			<view wx:if="{{item.positions.length}}" class="cu-item {{boxer[index] ? '' : 'piano-hidden-content'}}" wx:for="{{item.positions}}" wx:for-index="pindex" wx:for-item="child" wx:key="item">
				<view class="content flex justify-around">
					<!-- 岗位名称 -->
					<view class="flex-treble flex justify-center text-sm text-black ">
						<text>{{ child['name'] }}</text>
						<text class="cuIcon-info text-blue margin-bottom" bindtap="showModal" data-target="showPosDesc" data-sindex="{{index}}" data-pindex="{{pindex}}"></text>
					</view>
					<!-- 步进器 -->
					<view class="flex-twice flex-treble flex justify-center align-center text-lg">
						<text class="text-df"> {{child.joined}}/{{child.number}}</text>
					</view>
					<!-- 报名 -->
					<view wx:if="{{!isDead&&isJoin == 0}}" bindtap="showModal" data-target="toPintuan" data-timespan="{{index}}" data-position="{{pindex}}" class="flex-sub flex-treble flex justify-center align-center text-lg">
						<text class="cuIcon-friendaddfill xl text-green">+</text>
					</view>
					<!-- 取消报名 -->
					<view wx:elif="{{isJoin == 1&&index==idx[0]&&pindex==idx[1]}}" bindtap="showModal" data-target="toCancel" data-timespan="{{index}}" data-position="{{pindex}}" class="flex-sub flex-treble flex justify-center align-center text-lg">
						<text class="cuIcon-friendaddfill xl text-red">-</text>
					</view>
					<!-- 占个位置而已 -->
					<view wx:else class="flex-sub flex-treble flex justify-center align-center text-lg">
					</view>
				</view>

			</view>
		</view>
	</view>
	<view class="flex justify-between">
		<view class="content margin-left-xl">
			<text class="cuIcon-emojiflashfill text-pink"></text>
			<text class="text-grey">已加入</text>
		</view>
		<view class="margin-right-xl" bindtap="showModal" data-target="joinMembers">
			<view class="cu-avatar-group">
				<view wx:for="{{actions.joinMembers}}" wx:key="index" class="cu-avatar round sm" style="background-image:url(https://image.meiye.art/FiLUT-wb9DP0-dpxRQH19HJOOJOW?imageMogr2/thumbnail/450x/interlace/1);"></view>
				<!-- <view class="cu-avatar round sm" style="background-image:url(https://image.meiye.art/FlqKg5bugFQD5Qzm_QhGM7ET4Mtx?imageMogr2/thumbnail/450x/interlace/1);"></view>
				<view class="cu-avatar round sm" style="background-image:url(https://image.meiye.art/Fha6tqRTIwHtlLW3xuZBJj8ZXSX3?imageMogr2/thumbnail/450x/interlace/1);"></view>
				<view class="cu-avatar round sm" style="background-image:url(https://image.meiye.art/FhHGe9NyO0uddb6D4203jevC_gzc?imageMogr2/thumbnail/450x/interlace/1);"></view> -->
			</view>
			<text class="text-grey text-sm ">{{actions.outJoin}} 人</text>
		</view>
	</view>
</view>

<view class="static margin-sm radius-lg bg-white">
	<view class="solid-bottom text-lg text-bold padding text-black">服务信息</view>
	<view class="padding flex justify-between">
		<text class="">服务时间</text>
		<text class="text-df">{{constant.serviceTime}}</text>
	</view>
	<view class="padding flex  justify-between">
		<text class="">服务时长</text>
		<text class="text-df ">{{constant.hours+'小时'+constant.minutes+'分钟'}}</text>
	</view>
	<view class="padding flex  justify-between">
		<text class="">服务地点</text>
		<text class="text-df">{{actions.address.length>0?actions.address:"发布者未填写"}}</text>
	</view>
	<view class="padding flex  justify-between">
		<text class="">服务群体</text>
		<view wx:for="{{actions.groupTag}}" wx:key="index" class="cu-tag bg-pink">
			<view>
				#{{item}}
			</view>
		</view>
	</view>
	<view wx:if="{{actions.isSubsidy}}" class="padding flex  justify-between">
		<text class="text-red">服务补贴</text>
		<text class="text-df text-red">{{actions.subsidyAmount}}元</text>
	</view>
</view>

<!-- 服务简介 -->
<view wx:if="{{actions.intro}}" class="static margin-sm radius-lg bg-white">

	<view class=" radius-lg bg-white">
		<view class="solid-bottom text-lg text-bold padding text-black">服务简介</view>
		<view class="padding flex  justify-between">
			<text class="text-df">{{actions.intro}}
			</text>
		</view>
	</view>
</view>

<!-- 审核界面或者评价后负责人或管理员才可以看到以下信息 -->
<view wx:if="{{actions.introFile.length}}">
	<view wx:if="{{checkMode ||(actions.isFeedback&&isAdmin)}}" class=" radius-lg margin-sm bg-white">
		<view class="solid-bottom text-lg text-bold padding text-black">活动方案文件</view>
		<view class="flex padding-sm solid{{size?'s':''}}" wx:for="{{actions.introFile}}" wx:key="item" wx-index="index" bind:tap="openFile" data-target="{{index}}">
			<view class="padding-lr-xs ">
				<view class="cu-avatar lg round iconfont {{item.filetype=='.excel'?'icon-excel text-green':(item.filetype=='.ppt'?'icon-ppt text-red':((item.filetype=='.docx'||item.filetype=='.doc')?'icon-word text-blue':(item.filetype=='.pdf'?'icon-PDF text-red':'icon-icon-test')))}}">
				</view>
			</view>
			<view class="padding-xs text-xl text-black">
				<view class="text-sm">{{item.name}}</view>
				<view class="cu-tag round line-green sm">{{item.size}}</view>
			</view>
		</view>
	</view>

</view>

<!-- 审核的群二维码和i志愿图片 -->
<view wx:if="{{checkMode||(actions.isFeedback&&isAdmin)}}" class="static margin-sm radius-xl bg-white">
	<!-- 群二维码 -->
	<view wx:if="{{actions.qr_code.length}}">
		<view class="cu-bar bg-white margin-top-sm">
			<view class="text-lg text-bold padding text-black">
				群二维码
			</view>
		</view>
		<view class="cu-form-group">
			<view class="grid col-4 grid-square flex-sub">
				<view class="bg-img" wx:for="{{actions.qr_code}}" wx:key="index" catchtap="previewImage" data-index="{{index}}" data-url="0">
					<image src='{{actions.qr_code[index]}}' mode='aspectFill'></image>
				</view>
			</view>
		</view>

	</view>
	<!-- i志愿报名码 -->
	<view wx:if="{{actions.iZhiYuan.length}}">
		<view class="cu-bar bg-white ">
			<view class="text-lg text-bold padding text-black">
				i志愿报名码
			</view>
		</view>
		<view class="cu-form-group">
			<view class="grid col-4 grid-square flex-sub">
				<view class="bg-img" wx:for="{{actions.iZhiYuan}}" wx:key="index" bindtap="previewImage" data-index="{{index}}" data-url="1">
					<image src='{{actions.iZhiYuan[index]}}' mode='aspectFill'></image>
				</view>
			</view>
		</view>
	</view>
	<block wx:if="{{actions.isFeedback}}">
		<!-- 签到表 -->
		<view wx:if="{{actions.feedback.signInList.length}}" class=" ">
			<view class="cu-bar bg-white radius-xl">
				<view class="text-lg text-bold padding text-black">
					签到表
				</view>
			</view>
			<view class="cu-form-group">
				<view class="grid col-4 grid-square flex-sub">
					<view class="bg-img" wx:for="{{actions.feedback.signInList}}" wx:key="index" catchtap="previewImage" data-index="{{index}}" data-url="2">
						<image src='{{actions.feedback.signInList[index]}}' mode='aspectFill'></image>
					</view>
				</view>
			</view>

		</view>
		<!-- 活动照片 -->
		<view wx:if="{{actions.feedback.imgList.length}}">
			<view class="cu-bar bg-white ">
				<view class="text-lg text-bold padding text-black">
					活动照片
				</view>
			</view>
			<view class="cu-form-group">
				<view class="grid col-4 grid-square flex-sub">
					<view class="bg-img" wx:for="{{actions.feedback.imgList}}" wx:key="index" bindtap="previewImage" data-index="{{index}}" data-url="3">
						<image src='{{actions.feedback.imgList[index]}}' mode='aspectFill'></image>
					</view>
				</view>
			</view>
		</view>
	</block>
</view>

<view wx:if="{{checkMode||(actions.isFeedback&&isAdmin&&actions.feedback.membersInfo.length)}}" class="static margin-sm radius-xl bg-white">
	<view class="cu-bar bg-white margin-top-sm">
		<view class="text-lg text-bold padding text-black">
			志愿者信息
		</view>
	</view>

	<view class="table">
		<block>
			<view class="flex-sub">
				<view class="table-tr text-bold">
					<view class="table-th">姓名</view>
					<view class="table-th">出勤</view>
					<view class="table-th">表现</view>
					<view class="table-th">评价</view>
				</view>
				<scroll-view scroll-y='true' style="height:250rpx;">
					<view class="table-tr" wx:for="{{actions.feedback.membersInfo}}" wx:key="item" wx:for-index="index">
						<view class="table-th">{{item.info.name}}</view>
						<view class="table-th">{{item.isCome?'实到':'未到'}}</view>
						<view class="table-th">{{item.excellent?'优秀':'合格'}}</view>
						<view class="table-th" bindtap="showModal" data-target="showFeedback" data-index="{{index}}">点击查看</view>
					</view>
				</scroll-view>
			</view>
		</block>
	</view>
</view>


<button wx:if="{{actions.isFeedback&&isAdmin}}" class="bg-green" bindtap="myShowToast">导出excel</button>
