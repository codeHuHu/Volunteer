<view class="cu-list menu card-menu margin-tb-xl shadow-lg radius padding-lr-sm">
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>队伍名称:</text>
    </view>
    <view class="bg-oc-gray-2 radius margin-tb-xs solid">
      <input class="text-black padding-lr text-lg" placeholder="请输入你的队伍名称" bindinput="getTeamName" style="width:100%;height: 80rpx;"></input>
    </view>
  </view>
  <!-- <view class="content margin-bottom">
		<view class="text-lg text-black flex justify-between ">
			<text class="text-bold ">队伍类型:</text>
			<text class="text-lg text-gray">队伍类型说明</text>
		</view>
		<view class="cu-form-group bg-oc-gray-2 radius margin-tb-xs solid ">
			<picker bindchange="PickerChange" value="{{index}}" range="{{picker}}">
				<view class="picker flex text-black">
					{{teamCategory?teamCategory:'请选择队伍类型'}}
				</view>
			</picker>
		</view>
	</view> -->
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>队长姓名:</text>
    </view>
    <view class="bg-oc-gray-2 radius margin-tb-xs solid">
      <input class="text-black padding-lr text-lg" placeholder="请输入队长姓名" bindinput="getCaptainName" style="width:100%;height: 80rpx;"></input>
    </view>
  </view>
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>队长身份证号码:</text>
    </view>
    <view class="bg-oc-gray-2 radius margin-tb-xs solid">
      <input class="text-black padding-lr text-lg" placeholder="请输入队长身份证号码" bindinput="getCaptainNumber" style="width:100%;height: 80rpx;"></input>
    </view>
  </view>
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>队长手机号码:</text>
    </view>
    <view class="bg-oc-gray-2 radius margin-tb-xs solid">
      <input class="text-black padding-lr text-lg" placeholder="请输入队长手机号码" bindinput="getCaptainPhone" style="width:100%;height: 80rpx;"></input>
    </view>
  </view>
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>验证码:</text>
    </view>
    <view class=" radius margin-tb-xs flex">
      <view class="bg-oc-gray-2 flex-twice margin-right-sm  solid ">
        <input class="text-black padding-lr text-lg " placeholder="123456" bindinput="getName" style="width:90%;height: 80rpx;"></input>
      </view>
      <button class="flex-sub cu-btn bg-green shadow-blur round lg">获取验证码</button>
    </view>
  </view>
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>邮箱:</text>
    </view>
    <view class="bg-oc-gray-2 radius margin-tb-xs solid">
      <input class="text-black padding-lr text-lg" placeholder="请输入你的邮箱" bindinput="getMail" style="width:100%;height: 80rpx;"></input>
    </view>
  </view>
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>所在地区:</text>
    </view>
    <view class="cu-form-group bg-oc-gray-2 radius margin-tb-xs solid ">
      <picker class="" mode="region" bindchange="RegionChange" value="{{region}}" custom-item="{{customItem}}">
        <view class="picker text-black flex">
          {{region[0]}},{{region[1]}},{{region[2]}}
        </view>
      </picker>
    </view>
  </view>
  <!-- <view class="content margin-bottom">
		<view class="text-lg text-black text-bold ">
			<text>队伍申请函:</text>
		</view>
		<view class="bg-oc-gray-2 radius margin-tb-xs solid">
			<input class="text-black padding-lr text-lg" placeholder="请上传队伍申请函(未完善)" bindinput="getName" style="width:100%;height: 80rpx;"></input>
		</view>
	</view> -->
  <view class="content margin-bottom">
    <view class="text-lg text-black text-bold ">
      <text>队伍介绍:</text>
    </view>
    <view class="bg-oc-gray-2 radius margin-tb-xs solid">
      <textarea class="text-black padding-lr text-lg padding-tb" maxlength="-1" disabled="{{modalName!=null}}" bindinput="getTeamIntro" placeholder="服务目标/服务内容/服务对象" style="height: 200rpx;"></textarea>
    </view>
  </view>
  <!-- <view class="content margin-bottom-xl">
		<view class="text-lg text-black text-bold ">
			<text>申请原因:</text>
		</view>
		<view class="bg-oc-gray-2 radius margin-tb-xs solid">
			<textarea class="text-black padding-lr text-lg padding-tb" maxlength="-1" disabled="{{modalName!=null}}" bindinput="getReason" placeholder="申请原因不少于15个字" style="height: 150rpx;"></textarea>
		</view>
	</view> -->
  <view class="content">
    <checkbox class="round margin-right" checked="{{ischeck}}" bindtap="checkchange"></checkbox>
    <text class="">注册表示同意协议</text>
  </view>
  <view class="padding-xl">
    <button class="cu-btn block bg-cyan margin-tb-xl lg round " bindtap="teamRegister">
      注册
    </button>
  </view>
</view>

